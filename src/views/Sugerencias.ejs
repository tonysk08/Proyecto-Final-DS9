<%- include ('template/_header')%>
<div class="container">
    <div class="row">
      <div class="col-sm-9 col-md-7 col-lg-5 mx-auto">
        <div class="card card-signin my-5">
          <div class="card-body">
            <h3 class="card-title text-center">Sugerencias</h3>
            <hr class="my-4">
            <form class="form-signin">
              <div class="form-label-group">
                <label for="name">Nombre</label>
                <input type="text" id="name" class="form-control" required autofocus>
              </div>

              <div class="form-label-group">
                <label for="email">Correo Electr√≥nico</label>
                <input type="email" id="email" class="form-control" required>
              </div>

              <div class="col-xs-12">
                <div class="form-group">
                    <label for="comment">Mensaje</label>
                    <textarea class="form-control" rows="5" id="comment" placeholder="Describe tu sugerencia"></textarea>
                </div>
            </div>

              <button class="btn btn-lg btn-primary btn-block text-uppercase" type="submit">Enviar</button>
              
              
            </form>
          </div>
        </div>
      </div>
    </div>
  </div>
  <%- include ('template/_footer')%>